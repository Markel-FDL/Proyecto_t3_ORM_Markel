<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.mycompany.proyectos_t3_orm_markel_final.ProyectosEntity" table="proyectos" schema="proyecto_t3">
        <id name="codigo">
            <column name="codigo" sql-type="varchar(6)" length="6"/>
        </id>
        <property name="nombre">
            <column name="nombre" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="ciudad">
            <column name="ciudad" sql-type="varchar(40)" length="40" not-null="true"/>
        </property>
        <set name="gestionsByCodigo" inverse="true">
            <key>
                <column name="proyectos_codigo"/>
            </key>
            <one-to-many not-found="ignore" class="com.mycompany.proyectos_t3_orm_markel_final.GestionEntity"/>
        </set>
    </class>
</hibernate-mapping>